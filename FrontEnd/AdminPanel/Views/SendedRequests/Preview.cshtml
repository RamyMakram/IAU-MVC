							</table>
						</div>
					</div>
				</div>
			</div>
			<div class="card">
				<div class="card-body">
					<div class="row">
						@{
							var MostafidComment = (ViewBag.RequestTransaction as ICollection<IAUAdmin.DTO.Entity.Request_TransactionDTO>).Last();
							<div class="col-12">
								<div class="row">
									<div class="col-9" style="display:inline-flex;@(isAr?"justify-content:flex-start":"justify-content:flex-end")" id="CodeGenrateDiv">
										@foreach (var i in MostafidComment.Code.ToCharArray())
										{
											<input class="form-control Genratedcode en-font" value="@i" readonly disabled />
										}
									</div>
									<div class="col-3" style="direction:ltr;justify-content: center;align-items: center;display: flex;">
										<span style="padding: 12px;border-radius: 15px;background: darkgreen;color: white;">
