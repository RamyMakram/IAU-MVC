									<tr>
										<th key="t-middlename"></th>
										<th>@Model.Personel_Data.Middle_Name</th>
									</tr>
									<tr>
										<th key="t-lastname"></th>
										<th>@Model.Personel_Data.Last_Name</th>
									</tr>
									<tr>
										<th class="boldtitle" key="t-nationalty"></th>
									</tr>
									<tr>
										<th key="t-nationalty"></th>
