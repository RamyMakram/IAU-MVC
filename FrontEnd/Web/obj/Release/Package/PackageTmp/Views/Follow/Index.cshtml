@model IAU.DTO.Entity.FollowRequest_DTO
<div class="col-md-12 containt2">
    <div class="row">
        <div class="col-md-12">
            <div class="row">
                <div class="col-md-10">
                    <p>Track Request</p>
                </div>
                <div class="col-md-2">
                    <span> Help and <br />support</span> <img src="~/Design/img/HELP ICON.png" />
                </div>
            </div>
        </div>
        <div class="col-md-12 serial-input">
            <label>Insert request number</label>
            <input id="search_digit1" type="text" class="search" maxlength="1">
            <input id="search_digit2" type="text" class="search" maxlength="1">
            <input id="search_digit3" type="text" class="search" maxlength="1">
            <input id="search_digit4" type="text" class="search" maxlength="1">
            <input id="search_digit5" type="text" class="search" maxlength="1">
            <input id="search_digit6" type="text" class="search" maxlength="1">
            <input id="search_digit7" type="text" class="search" maxlength="1">
            <input id="search_digit8" type="text" class="search" maxlength="1">
            <input id="search_digit9" type="text" class="search" maxlength="1">
            <input id="search_digit10" type="text" class="search" maxlength="1">
            <input id="search_digit11" type="text" class="search" maxlength="1">
            <input id="search_digit12" type="text" class="search" maxlength="1">
            <input id="search_digit13" type="text" class="search" maxlength="1">
            <button onclick="showState()">Show</button>
            <button onclick="searchByPhone()">Search For Previous Request</button>
        </div>
        <div class="col-md-12" id="div_Follow" style="display:none">
            <div class="row">
                <div class="col-md-3">
                    <label>Shipped via : Twasul </label>
                    <span>
                        <span class="active">
                            <span></span>
                        </span>
                        <br />
                        <span>
                            <span></span>
                        </span>
                    </span>
                </div>
                <div class="col-md-3">
                    <label>Processing<br /> location:</label>
                    <span id="spn_location"></span>
                </div>
                <div class="col-md-3">
                    <label>status</label>
                    <span id="spn_status"></span>
                </div>
                <div class="col-md-3">
                    <label>Expected <br /> delivery date</label>
                    <span id="spn_deliverydate"></span>
                </div>
                <div class="col-md-12">
                    <div class="row">
                        <div class="col-md-2 progress-sucess-state">
                            <div class="border-state">
                                <img src="~/Design/img/follow/confirmed.png" />
                            </div>
                            <span>Dispatching</span>
                            <span>Request</span>
                        </div>
                        <div class="col-md-1 progress-sucess-state">
                            <span></span>
                        </div>
                        <div class="col-md-2 progress-sucess-state">
                            <div class="border-state">
                                <img src="~/Design/img/follow/distriputing.png" />
                            </div>
                            <span>Sorting</span>
                            <span>Request</span>
                        </div>
                        <div class="col-md-1">
                            <span></span>
                        </div>
                        <div class="col-md-2">
                            <div class="border-state">
                                <img src="~/Design/img/follow/png.png" />
                            </div>
                            <span>Processing</span>
                            <span>Request</span>
                        </div>
                        <div class="col-md-1">
                            <span></span>
                        </div>
                        <div class="col-md-2">
                            <div class="border-state">
                                <img src="~/Design/img/follow/PRequest.png" />
                            </div>
                            <span>Dispatching</span>
                            <span>Progressed</span>
                            <span>request</span>
                        </div>
                        <div class="col-md-1">
                            <span></span>
                        </div>
                        <div class="col-md-2">
                            <div class="border-state">
                                <img src="~/Design/img/follow/Drived.png" />
                            </div>
                            <span>Processed</span>
                            <span>request</span>
                            <span>delivered</span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="modal fade" id="phoneSearch" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Display related requests for this phone </h5>
                @*<button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>*@<button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <div class="alert alert-danger" id="verficationFeedback" style="display:none">
                    PLZ, Enter Correct Verification Code.
                </div>
                <div class="alert alert-danger" id="GenralError" style="display:none">
                    There Is General Error
                </div>
                <form>
                    <div class="mb-3 phone-input">
                        <label for="verficationCode" class="col-form-label">please insert your phone Number</label>
                        <input type="text" class="form-control" autocomplete="off">
                    </div>
                </form>
            </div>
            <div class="modal-footer text-center">
                <button type="button" onclick="" class="btn btn-success" id="Comfirm-Digits">Show</button>
            </div>
        </div>
    </div>
</div>
<script src="~/Design/JS/jquery.js"></script>

<script src="~/Design/JS/follow.js"></script>


