@model IAUAdmin.DTO.Entity.UnitsDTO
@{
	ViewBag.Title = "Active Category";
	ViewBag.pTitle = "Active Category";
	ViewBag.pageTitle = "Categories";
	ViewBag.arTitle = "تفعيل فئة";
	ViewBag.arpageTitle = "تفعيل فئة";
	ViewBag.arpTitle = "الفئات الادارية";
	Layout = "~/Views/Shared/_Layout.cshtml";
}
<!-- select2 css -->
<link href="~/assets/libs/select2/css/select2.min.css" rel="stylesheet" type="text/css" />

<!-- dropzone css -->
<link href="~/assets/libs/dropzone/min/dropzone.min.css" rel="stylesheet" type="text/css" />
<h2 key="t-deactive">Deactive</h2>

<div class="row">
	@using (Html.BeginForm("Active", "Units", new { Id = Model.Units_ID, x = "" }, FormMethod.Post, new { @class = "actionForm" }))
	{
		<div class="alert alert-success dt-center p-14" style="text-align:center;width:100%" key="t-active-units">
			Do u want to Active this User?
		</div>

		<button type="submit" key="t-save" class="btn btn-primary mr-1 waves-effect waves-light">Save Changes</button>
		@Html.ActionLink("Cancel", "Home", "Units", null, new { @class = "btn btn-secondary waves-effect", @key = "t-cancel" })
	}
</div>
<script src="~/assets/libs/jquery/jquery.min.js"></script>
<script src="~/assets/libs/bootstrap/js/bootstrap.bundle.min.js"></script>
<script src="~/assets/libs/metismenu/metisMenu.min.js"></script>
<script src="~/assets/libs/simplebar/simplebar.min.js"></script>
<script src="~/assets/libs/node-waves/waves.min.js"></script>
<script src="~/assets/js/lang/jquery.multiLanguage.js"></script>

<!-- select 2 plugin -->
<script src="~/assets/libs/select2/js/select2.min.js"></script>

<!-- dropzone plugin -->
<script src="~/assets/libs/dropzone/min/dropzone.min.js"></script>

<!-- init js -->
<script src="~/assets/js/pages/ecommerce-select2.init.js"></script>

<!-- App js -->
<script src="~/assets/js/app.js"></script>
