									</tr>
									<tr>
										<th key="t-email"></th>
										<th class="en-font">@Model.Personel_Data.Email</th>
									</tr>
									<tr>
										<th key="t-phone"></th>
										<th class="en-font" style="direction:ltr;text-align:start;@(isAr ? "text-align: end !important;" : "")">@Model.Personel_Data.Mobile</th>
